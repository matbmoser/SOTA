#!/usr/bin/env bash

function getPythonFilePID(){
    echo "$(ps -ef | grep "python3" | grep ${1} | awk {'print$2'})"
}
function openDefaultServer(){
    . ${DEVICE_MANAGER}/ServerManager.py --default > ${1}
}
function openServer(){
    . ${DEVICE_MANAGER}/ServerManager.py -n ${1} -ip ${2} -port ${3} 
}